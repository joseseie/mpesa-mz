{
    "name": "calvinchiulele/mpesa-mz",
    "description": "Unofficial laravel package for Vodacom Mozambique M-Pesa",
    "type": "library",
    "require": {
        "abdulmueid/mpesa": "^1.0",
        "php": "^7.0",
        "illuminate/support": "^5.5",
        "vlucas/phpdotenv": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Calvin Chiulele",
            "email": "cchiulele@protonmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "calvinchiulele\\MPesaMz\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "calvinchiulele\\MPesaMz\\Providers\\MPesaMzServiceProvider"
            ],
            "aliases": {
                "MPesaMz": "calvinchiulele\\MPesaMz\\Facades\\MPesaMzFacade"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
